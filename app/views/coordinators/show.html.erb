</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @coordinator.name %></dd>

      <dt>Email address</dt>
      <dd><%= @coordinator.email_address %></dd>

      <dt>Phone number</dt>
      <dd><%= @coordinator.phone_number %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/coordinators" class="btn btn-primary">
        Back
      </a>
      <a href="/coordinators/<%= @coordinator.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_coordinator/<%= @coordinator.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

