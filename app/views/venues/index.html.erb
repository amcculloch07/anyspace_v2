<div class="page-header">
  <h1>
    Venues
    <a href="/venues/new" class="btn btn-lg btn-success">New Venue</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th>Website</th>
        <th>Capacity</th>
        <th>Description</th>
        <th>Drink 1</th>
        <th>Drink 2</th>
        <th>Drink 3</th>
        <th>Drink 4</th>
        <th>Drink 5</th>
        <th>Food 1</th>
        <th>Food 2</th>
        <th>Food 3</th>
        <th>Food 4</th>
        <th>Food 5</th>
        <th>Drink 1 price</th>
        <th>Drink 2 price</th>
        <th>Drink 3 price</th>
        <th>Drink 4 price</th>
        <th>Drink 5 price</th>
        <th>Food 1 price</th>
        <th>Food 2 price</th>
        <th>Food 3 price</th>
        <th>Food 4 price</th>
        <th>Food 5 price</th>
        <th>Amenity 1</th>
        <th>Amenity 2</th>
        <th>Amenity 3</th>
        <th>Amenity 4</th>
        <th>Amenity 5</th>
        <th>Amenity 6</th>
        <th>Amenity 7</th>
        <th>Amenity 8</th>
        <th>Rule 1</th>
        <th>Rule 2</th>
        <th>Rule 3</th>
        <th>Rule 4</th>
        <th>Rule 5</th>
        <th>Rule 6</th>
        <th>Coordinator</th>
        <th>Title</th>
        <th>Actions</th>
      </tr>

      <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.address %></td>
        <td><%= venue.city %></td>
        <td><%= venue.state %></td>
        <td><%= venue.zip %></td>
        <td><%= venue.website %></td>
        <td><%= venue.capacity %></td>
        <td><%= venue.description %></td>
        <td><%= venue.drink_1 %></td>
        <td><%= venue.drink_2 %></td>
        <td><%= venue.drink_3 %></td>
        <td><%= venue.drink_4 %></td>
        <td><%= venue.drink_5 %></td>
        <td><%= venue.food_1 %></td>
        <td><%= venue.food_2 %></td>
        <td><%= venue.food_3 %></td>
        <td><%= venue.food_4 %></td>
        <td><%= venue.food_5 %></td>
        <td><%= venue.drink_1_price %></td>
        <td><%= venue.drink_2_price %></td>
        <td><%= venue.drink_3_price %></td>
        <td><%= venue.drink_4_price %></td>
        <td><%= venue.drink_5_price %></td>
        <td><%= venue.food_1_price %></td>
        <td><%= venue.food_2_price %></td>
        <td><%= venue.food_3_price %></td>
        <td><%= venue.food_4_price %></td>
        <td><%= venue.food_5_price %></td>
        <td><%= venue.amenity_1 %></td>
        <td><%= venue.amenity_2 %></td>
        <td><%= venue.amenity_3 %></td>
        <td><%= venue.amenity_4 %></td>
        <td><%= venue.amenity_5 %></td>
        <td><%= venue.amenity_6 %></td>
        <td><%= venue.amenity_7 %></td>
        <td><%= venue.amenity_8 %></td>
        <td><%= venue.rule_1 %></td>
        <td><%= venue.rule_2 %></td>
        <td><%= venue.rule_3 %></td>
        <td><%= venue.rule_4 %></td>
        <td><%= venue.rule_5 %></td>
        <td><%= venue.rule_6 %></td>
        <td><%= venue.coordinator_id %></td>
        <td><%= venue.title %></td>
        <td>
          <a href="/venues/<%= venue.id %>" class="btn btn-primary">Show</a>
          <a href="/venues/<%= venue.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_venue/<%= venue.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
